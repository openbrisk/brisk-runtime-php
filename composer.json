{
    "name": "openbrisk/brisk-runtime-php",
    "description": "An OpenBrisk runtime for functions written in PHP.",
    "keywords": ["faas", "serverless", "functions", "runtime", "php"],
    "homepage": "http://github.com/openbrisk/brisk-runtime-php",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "slim/slim": "^3.1",
        "monolog/monolog": "^1.17"
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public index.php"
    }
}
